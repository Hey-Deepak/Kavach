<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dc.kavach">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.REQUEST_DELETE_PACKAGES"></uses-permission>
    <uses-permission android:name="android.permission.QUERY_ALL_PACKAGES"></uses-permission>
    <application
        android:name="com.dc.kavach.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_tank_app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kavach">
        <activity
            android:name="com.dc.kavach.ui.main.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Kavach.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>